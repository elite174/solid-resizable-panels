    "solid-js": "^1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "babel-preset-solid": "^1.7.1",
    "rollup": "^3.10.1",
